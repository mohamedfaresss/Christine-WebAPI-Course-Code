{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    // Replace with your local connection string in appsettings.Development.json
    "cs": "Your-Connection-String-Here"
  },

  "JWT": {
    // Replace with your real key in Development environment only
    "SecurityKey": "Your-Security-Key-Here",

    // Replace with your actual Audience URL
    "AudianceIP": "Your-Audience-URL",

    // Replace with your actual Issuer URL
    "IssuerIP": "Your-Issuer-URL"
  },

  "AllowedHosts": "*"
}
